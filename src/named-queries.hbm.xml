<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <query name="findInvoicesWithQnt">from Invoice as invoice where invoice.Quantity=:qnt</query>
</hibernate-mapping>